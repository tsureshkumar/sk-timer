    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "target": "es2015",
    "module": "es2015",
    "strict": false,
    "jsx": "react"
